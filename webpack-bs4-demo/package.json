{
  "name": "test-aurelia-new",
  "description": "Aurelia-Slickgrid",
  "version": "2.0.0",
  "homepage": "https://github.com/ghiscoding/aurelia-slickgrid",
  "bugs": {
    "url": "https://github.com/ghiscoding/aurelia-slickgrid/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/ghiscoding/aurelia-slickgrid"
  },
  "license": "MIT",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "build": "nps build",
    "prebuild-demo": "npm run del-ghpage",
    "build-demo": "npm run build",
    "postbuild-demo": "npm-run-all copy-ghpage:dist copy:circleci-config",
    "copy:circleci-config": "cross-env copyfiles -f .circleci/**/*.* ../../aurelia-slickgrid-bs4-GhPages/.circleci/",
    "copy-ghpage:dist": "cross-env copyfiles --up 1 dist/**/*.* ../../aurelia-slickgrid-bs4-GhPages",
    "del-ghpage": "cross-env rimraf ../../aurelia-slickgrid-bs4-GhPages/**/*.*"
  },
  "dependencies": {
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-http-client": "^1.3.1",
    "aurelia-i18n": "^2.3.2",
    "aurelia-polyfills": "^1.3.4",
    "aurelia-slickgrid": "^2.10.1",
    "bluebird": "^3.5.4",
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "i18next-xhr-backend": "^1.5.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.11.4",
    "@types/webpack": "^4.4.14",
    "aurelia-cli": "^0.35.1",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-pal-nodejs": "^1.1.1",
    "aurelia-protractor-plugin": "^1.0.6",
    "aurelia-testing": "^1.0.0-beta.4.0.0",
    "aurelia-tools": "^2.0.0",
    "aurelia-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-rename": "^1.4.0",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.3",
    "minimatch": "^3.0.4",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.5",
    "nps": "^5.9.3",
    "nps-utils": "^1.7.0",
    "opn": "^5.4.0",
    "protractor": "^5.4.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "through2": "^2.0.3",
    "ts-jest": "^23.10.3",
    "ts-loader": "^5.2.1",
    "ts-node": "^7.0.1",
    "typescript": "^2.7.2",
    "uglify-js": "^3.4.9",
    "url-loader": "^1.1.1",
    "vinyl-fs": "^3.0.3",
    "wait-on": "^3.1.0",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "latest",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx)",
      "!*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "!*.d.ts",
      "example-data.js"
    ],
    "setupFiles": [
      "<rootDir>/test/jest-pretest.ts"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "main": "dist/app.bundle.js"
}
